{
    "name": "",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check .",
        "format": "prettier --write .",
        "devext": "nodemon --watch src --watch static --exec \"vite build\" -e json,ts,css,html,svelte",
        "openapi": "openapi --input http://127.0.0.1:8000/openapi.json --output ./src/lib/api/openapi --useOptions"
    },
    "devDependencies": {
        "@skeletonlabs/skeleton": "2.9.1",
        "@skeletonlabs/tw-plugin": "0.4.0",
        "@sveltejs/adapter-auto": "^3.2.0",
        "@sveltejs/kit": "^2.5.5",
        "@sveltejs/vite-plugin-svelte": "^3.1.0",
        "@tailwindcss/forms": "0.5.7",
        "@tailwindcss/typography": "0.5.12",
        "@types/chrome": "^0.0.266",
        "@types/node": "20.12.7",
        "autoprefixer": "10.4.19",
        "nodemon": "^3.1.0",
        "openapi-typescript-codegen": "^0.29.0",
        "postcss": "8.4.38",
        "prettier": "^3.2.5",
        "prettier-plugin-svelte": "^3.2.2",
        "svelte": "^4.2.13",
        "svelte-check": "^3.6.9",
        "sveltekit-adapter-chrome-extension": "^2.0.1",
        "tailwindcss": "3.4.3",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5",
        "vite": "^5.2.8",
        "vite-plugin-tailwind-purgecss": "0.3.0"
    },
    "type": "module",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.5.2"
    }
}
